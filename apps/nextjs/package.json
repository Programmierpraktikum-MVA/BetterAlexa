{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "build:ext": "pnpm with-env bash build-ext.sh",
    "clean": "git clean -xdf .next .turbo node_modules",
